<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/background7"
    tools:context=".AboutUsFragment">

    <TextView
        android:id="@+id/editTextTextMultiLine"
        android:layout_width="match_parent"
        android:layout_height="508dp"
        android:layout_marginTop="220dp"
        android:ems="10"
        android:textAlignment="center"
        android:textSize="24dp"
        android:textStyle="bold"
        android:text="HEllO EVERYONE , THANKS FOR CHOSING OUR APPLICATION ALSO FOR YOUR TRUST. WE ARE FOCUSINGON WHAT WE PROVIDE,QUALITY OF PRODUCT . WE ASSURE YOU THAT WE PROVIDE THE BEST SERVICE TO YOU . THANK YOU..."
        android:inputType="textMultiLine" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="174dp"
        android:layout_height="72dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="110dp"
        android:fontFamily="cursive"
        android:text="OHC"
        android:textAlignment="center"
        android:textColor="@android:color/holo_orange_dark"
        android:textColorHighlight="@color/teal_200"
        android:textSize="60sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="267dp"
        android:layout_height="27dp"
        android:layout_marginTop="85dp"
        android:layout_marginLeft="60dp"
        android:text="ONLINE HEALTH CARE"
        android:textAlignment="center"
        android:textColor="@android:color/holo_red_light"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />


    <TextView
        android:id="@+id/textView7"
        android:layout_width="258dp"
        android:layout_height="59dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="110dp"
        android:text="ABOUT US"
        android:textAlignment="center"
        android:textColor="?android:attr/colorFocusedHighlight"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />



</FrameLayout>